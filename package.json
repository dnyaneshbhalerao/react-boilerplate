{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "'Boilerplate code for React SPA'",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "start": "webpack-dev-server",
    "update-packages": "node clear-package-dependency.js && rm -rf node_modules && npm update --save-dev && npm update --save"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dnyaneshbhalerao/react-boilerplate.git"
  },
  "author": "Dnyaneshwar Bhalerao",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dnyaneshbhalerao/react-boilerplate/issues"
  },
  "homepage": "https://github.com/dnyaneshbhalerao/react-boilerplate#readme",
  "dependencies": {
    "babel-polyfill": "*",
    "react": "^16.3.2",
    "react-hot-loader": "^4.2.0",
    "react-router": "*"
  },
  "devDependencies": {
    "babel": "*",
    "babel-cli": "*",
    "babel-core": "*",
    "babel-loader": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "css-loader": "*",
    "eslint": "*",
    "eslint-plugin-react": "*",
    "html-webpack-plugin": "*",
    "react-dom": "*",
    "style-loader": "*",
    "uglifyjs-webpack-plugin": "*",
    "webpack": "*",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "*"
  }
}
