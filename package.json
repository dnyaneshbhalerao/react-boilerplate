{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "'Boilerplate code for React SPA'",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "start": "webpack-dev-server",
    "update-packages": "node clear-package-dependency.js && rm -rf node_modules && npm update --save-dev && npm update --save"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dnyaneshbhalerao/react-boilerplate.git"
  },
  "author": "Dnyaneshwar Bhalerao",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dnyaneshbhalerao/react-boilerplate/issues"
  },
  "homepage": "https://github.com/dnyaneshbhalerao/react-boilerplate#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "react": "^16.3.2",
    "react-hot-loader": "^4.2.0",
    "react-router": "^4.2.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.2",
    "html-webpack-plugin": "^3.2.0",
    "react-dom": "^16.3.2",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
